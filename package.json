    "@types/prettier": "^2.7.0",
    "@types/swagger2openapi": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "ts-node": "^10.7.0",
