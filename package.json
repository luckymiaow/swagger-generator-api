    "ora": "5.4.1",
    "prettier": "^2.7.1",
    "swagger2openapi": "^7.0.8",
    "typescript-string-operations": "^1.4.1",
    "unconfig": "^0.3.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.5",
    "@types/fs-extra": "^9.0.13",
    "@types/handlebars-helpers": "^0.5.4",
    "@types/inquirer": "^9.0.4",
